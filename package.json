{
  "name": "restaurant-api",
  "version": "0.0.1",
  "description": "Restaurant API for Test",
  "main": "index.js",
  "scripts": {
    "dev": "SET NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "postinstall": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node ./dist/index.js",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Stephen Omoarukhe",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.18.2",
    "nodemon": "^1.17.1"
  }
}
